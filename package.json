    "crypto",
    "bitcoin",
    "ethereum",
    "solana",
    "dogecoin",
    "trust-wallet",
    "web3",
    "bip39",
    "bip44",
    "bip84",
    "hd-wallet",
    "erc20",
    "spl-token",
    "eip712",
    "eip191",
    "transaction-signing"
